{
  "name": "WordList",
  "description" : "A vocabulary list app",
  "version": "1.0",
  "browser_action": {
    "default_popup": "WordList.html",
    "default_icon": "extension_logo.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens WordList.html"
    }
  }
}
